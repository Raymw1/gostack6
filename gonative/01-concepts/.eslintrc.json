{
    "parser": "babel-eslint",
    "env": {
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react", "prettier"
    ],
    "rules": {
      "react/jsx-filename-extension": ["error", {"extensions": [".js", ".jsx"]}],
      "import/prefer-default-export": "off",
      "prettier/prettier": "error"
    }
}
