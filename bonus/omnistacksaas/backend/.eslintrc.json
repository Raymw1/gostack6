{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard", "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "error"
    },
    "globals": {
      "use": true
    }
}
