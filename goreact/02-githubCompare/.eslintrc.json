{
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  "extends": ["react-app", "standard"],
  "plugins": ["import", "jsz-a11y", "react"],
  "rules": {
    "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }],
    "import/prefer-default-export": "off",
    "react/jsx-one-expression-per-line": "off"
  }
}